      android:usesCleartextTraffic="true"
      android:theme="@style/AppTheme">
      <meta-data
      android:name="com.google.android.geo.API_KEY"
      android:value="AIzaSyDgdfbt2kBGOTm-Y6ESIC-D6uLhcshrHfo"/>
